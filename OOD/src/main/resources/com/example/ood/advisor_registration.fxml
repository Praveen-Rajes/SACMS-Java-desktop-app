<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="736.0" prefWidth="1200.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.ood.RegistrationController">
    <children>
        <Label alignment="TOP_CENTER" layoutX="418.0" layoutY="23.0" prefHeight="18.0" prefWidth="363.0" text="Club Advisor Registration Form" textAlignment="CENTER" textFill="#071e7e">
            <font>
                <Font name="System Bold" size="18.0" />
            </font>
        </Label>
        <Button fx:id="submitButton2" layoutX="567.0" layoutY="676.0" mnemonicParsing="false" onAction="#onSubmitButton2Click" style="-fx-background-color: #071e7e;" text="Submit" textFill="#e3e5e7">
            <font>
                <Font name="System Bold" size="18.0" />
            </font>
        </Button>
        <Pane layoutX="26.0" layoutY="66.0" prefHeight="593.0" prefWidth="1146.0" style="-fx-border-color: #071e7e;">
            <children>
                <TextField fx:id="advisorIdField" layoutX="692.0" layoutY="29.0" prefHeight="26.0" prefWidth="307.0" promptText="Enter Advisor ID" />
                <TextField fx:id="advisorFNameField" layoutX="692.0" layoutY="108.0" prefHeight="26.0" prefWidth="307.0" promptText="Enter First Name" />
                <TextField fx:id="advisorLNameField" layoutX="692.0" layoutY="193.0" prefHeight="26.0" prefWidth="307.0" promptText="Enter Last Name" />
                <TextField fx:id="advisorDOBField" layoutX="692.0" layoutY="284.0" prefHeight="26.0" prefWidth="307.0" promptText="Enter Date of Birth" />
                <Label layoutX="42.0" layoutY="42.0" prefHeight="18.0" prefWidth="57.0" text="Image :">
                    <font>
                        <Font size="14.0" />
                    </font>
                </Label>
                <TextField fx:id="advisorGenderField" layoutX="692.0" layoutY="359.0" prefHeight="26.0" prefWidth="307.0" promptText="Enter Gender" />
                <TextField fx:id="advisorAddressField" layoutX="161.0" layoutY="500.0" prefHeight="26.0" prefWidth="307.0" promptText="Enter Home Address" />
                <TextField fx:id="advisorPhoneField" layoutX="692.0" layoutY="500.0" prefHeight="26.0" prefWidth="307.0" promptText="Enter Phone Number" />
                <Label layoutX="24.0" layoutY="-15.0" style="-fx-background-color: white;" text="Personal Details" textFill="#071e7e">
                    <font>
                        <Font size="18.0" />
                    </font>
                </Label>
                <Button fx:id="selectimagebutton2" layoutX="468.0" layoutY="360.0" mnemonicParsing="false" onAction="#onSelectImageButton2Click" text="Select Image " />
                <ImageView fx:id="advisorImageField" fitHeight="306.0" fitWidth="291.0" layoutX="122.0" layoutY="79.0" pickOnBounds="true" smooth="false">
                    <image>
                        <Image url="@../../../images/uploadimageicon.png" />
                    </image>
                </ImageView>
            </children>
        </Pane>
    </children>
</AnchorPane>
